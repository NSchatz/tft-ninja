{
  "dependencies": {
    "concurrently": "^7.4.0",
    "dotenv": "^16.0.3",
    "errorhandler": "^1.5.1",
    "fs": "^0.0.1-security",
    "pg": "^8.8.0",
    "pg-connection-string": "^2.5.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "client": "cd frontend && npm start",
    "server": "cd backend && npm start",
    "client-format": "cd frontend && npm run format",
    "backend-format": "cd backend && npm run format",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "format": "concurrently \"npm run backend-format\" \"npm run client-format\""
  },
  "devDependencies": {
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1"
  }
}
